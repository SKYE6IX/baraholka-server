  "scripts": {
    "build": "rimraf dist && npx tsc && tsc-alias && npm run sentry:sourcemaps",
    "start": "node dist/app.js",
    "dev": "concurrently \"tsc -w\" \"tsc-alias -w\" \"nodemon dist/app.js\"",
    "lint": "npx eslint **/*.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org skyetech --project remarket-backend ./dist && sentry-cli sourcemaps upload --org skyetech --project remarket-backend ./dist"
  },
